      <view class='title bold text-white text-l w100 text-center pt30'>{{banner.banner_title}}</view>
      <view class='content notice_content text-white  w100 pt30' scroll-y="{{!is_cos_open&&!is_run_open}}"  scroll-top="{{scrollTop}}">
